FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      controlDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

application         pimpleFoam;
startFrom           latestTime;
endTime             0.5;
stopAt              endTime;
deltaT              1;
writeControl        adjustableRunTime;
writeInterval       0.01;
writeFormat         ascii;
writePrecision      6;
writeCompression    uncompressed;
timeFormat          general;
timePrecision       6;
runTimeModifiable   yes;
adjustTimeStep      yes;
maxCo               2;
maxDeltaT           1;

